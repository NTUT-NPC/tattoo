# The bundle identifier of your app
FASTLANE_APP_IDENTIFIER=club.ntut.tattoo

# Shared Credentials Repository Configuration
# -------------------------------------------
# These settings are used by tool/credentials.dart to fetch encrypted configuration
# files (Firebase configs, Android keystores, etc.) from a private Git repository.

# The URL of the private credentials Git repository
MATCH_GIT_URL=

# The branch to use in the credentials repository (usually 'main')
MATCH_GIT_BRANCH=main

# Basic Authorization header for Git operations (GitHub PAT)
# Generate with: echo -n your_github_username:your_personal_access_token | base64
# Note: On Windows PowerShell, use: [Convert]::ToBase64String([System.Text.Encoding]::UTF8.GetBytes("user:token"))
MATCH_GIT_BASIC_AUTHORIZATION=

# Master password used to decrypt the files in the credentials repository
MATCH_PASSWORD=

# iOS Configuration (Fastlane)
# ----------------------------

# Apple Developer account email
FASTLANE_APPLE_ID=

# Apple Developer Team ID (e.g., G582M7KCHL)
FASTLANE_TEAM_ID=

# App Store Connect API Key (for TestFlight uploads)
# Create at: https://appstoreconnect.apple.com/access/integrations/api

# Key ID from App Store Connect (e.g., ABC123XYZ)
APP_STORE_CONNECT_API_KEY_KEY_ID=

# Issuer ID from App Store Connect Keys page (UUID format)
APP_STORE_CONNECT_API_KEY_ISSUER_ID=

# Base64 encoded content of the .p8 file
# Linux/macOS: base64 -i AuthKey_ABC.p8
# Windows: [Convert]::ToBase64String([IO.File]::ReadAllBytes("AuthKey_ABC.p8"))
APP_STORE_CONNECT_API_KEY_KEY=

# Controls whether to enable production features like Firebase in iOS beta builds
RELEASE_MODE=false

# Android Configuration (Fastlane)
# --------------------------------

# Path to Android SDK (optional, fastlane usually finds it)
# ANDROID_HOME=/path/to/android/sdk

# Firebase App ID for Android (from Firebase Console)
# Used for Firebase App Distribution
FIREBASEAPPDISTRO_APP=
