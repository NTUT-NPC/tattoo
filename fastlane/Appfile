# iOS configuration
app_identifier(ENV["FASTLANE_APP_IDENTIFIER"]) # The bundle identifier of your app
apple_id(ENV["FASTLANE_APPLE_ID"]) # Your Apple Developer Portal username
team_id(ENV["FASTLANE_TEAM_ID"]) # Your Apple Developer Team ID

# Android configuration
package_name(ENV["FASTLANE_APP_IDENTIFIER"]) # e.g. com.krausefx.app

